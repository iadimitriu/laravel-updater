{
    "name": "iadimitriu/laravel-updater",
    "description": "Laravel updater package",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandru Dimitriu",
            "email": "alexandru@dimitriu.io"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "~5.5.43|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/database": "~5.5.43|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/filesystem": "~5.5.43|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.5.43|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Iadimitriu\\LaravelUpdater\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Iadimitriu\\LaravelUpdater\\UpdaterServiceProvider"
            ]
        }
    }
}
